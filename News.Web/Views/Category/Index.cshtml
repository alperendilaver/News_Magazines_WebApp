@model List<Category>

<div class="container">
    <div class="row">
        @foreach (var category in Model)
        {
            <div class="col-6 col-md-4 col-lg-2 kategori"><a asp-action="Detail" asp-controller="Category" asp-route-id="@category.CategoryId">@category.CategoryName</a></div>
        }
    </div>
</div>


<style>
    .kategori {
        background-color: #f1f1f1;
        border: 1px solid #ccc;
        text-align: center;
        padding: 20px;
        margin: 5px 0;
    }
</style>